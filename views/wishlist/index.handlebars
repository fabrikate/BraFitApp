<h2>Bra Wish list</h2>
<p>Content will go here</p>
<a href='/survey'>Survey </a>
<a href='/wishlist/new'>Add a Bra to your wishlist </a>
<p id='userID'>User: {{user.email}}</p>
<p>Recommended Size: {{user.braSize}}</p>
<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Style</th>
      <th>Size</th>
      <th>Edit</th>
      <th>Remove</th>
    </tr>
  </thead>
  <tbody>
    {{#each user.wishList}}
    <tr>
      <td>{{brandName}}</td>
      <td>{{brandStyle}}</td>
      <td>{{size}}</td>
      <td><a href='/wishlist/{{id}}/edit'>âˆš</a></td>
      <td><form action='/wishlist/{{id}}/?_method=delete' method='POST'><button>x</button></form></td>
    </tr>
    {{/each}}
  </tbody>
</table>

